                }
            });
        });

        $(document).ready(function() {
            $(document).on("totalUpdate", function() {
                var partsTotal = $('#parts_total_preview').text();
                var labourTotal = $('#labour_total_preview').text();
                var paintTotal = $('#paint_total_preview').text();
                var extrasTotal = $('#extras_total_preview').text();
                var estimateTotal = Number(partsTotal) + Number(labourTotal) + Number(paintTotal) + Number(extrasTotal);
                estimateTotal = estimateTotal.toFixed(2);
                $('#estimate_total_preview').text(estimateTotal);
