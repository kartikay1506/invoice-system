    <script>
        $(document).ready(function() {
            $(document).on("partsUpdate", function() {
                var partsData = [];
                var partsTotal = 0;
                $('#parts_table tbody tr').each(function(index, value) {
                    var row =  GetPartsRow(index);
                    partsData.push(row);
                });
                $('#partsData').val(JSON.stringify(partsData));
                $('#parts_total_preview').text(partsTotal);
                $(this).trigger("totalUpdate");
                    // Read the row into an object
                function GetPartsRow(rowNum)
                {
                    var row = $('#parts_table tbody tr').eq(rowNum);

                    var myObject = {};

                    var partCost = row.find('td:eq(3)').text();
                    partCost = partCost.split(",").join("");
                    var temp = Number(partCost);
                    partsTotal = Number(partsTotal) + temp;
                    partsTotal = partsTotal.toFixed(2);
                    
                    myObject.PartNumber = row.find('td:eq(0)').text();
                    myObject.PartName = row.find('td:eq(1)').text();
                    myObject.Quantity = row.find('td:eq(2)').text();
                    myObject.Amount = row.find('td:eq(3)').text();
                    return myObject;
                }

            });
        });

        $(document).ready(function() {
            $(document).on("labourUpdate", function() {
                var labourData = [];
                var labourTotal = 0;
                $('#labour_table tbody tr').each(function(index, value) {
                    var row =  GetLabourRow(index);
                    labourData.push(row);
                });
                $('#labourData').val(JSON.stringify(labourData));
                $('#labour_total_preview').text(labourTotal);
                $(this).trigger("totalUpdate");
                    // Read the row into an object
                function GetLabourRow(rowNum)
                {
                    var row = $('#labour_table tbody tr').eq(rowNum);

                    var myObject = {};
                    var labourCost = row.find('td:eq(1)').text();
                    labourCost = Number(labourCost);
                    labourTotal = Number(labourTotal) + labourCost;
                    labourTotal = labourTotal.toFixed(2);
                    myObject.Description = row.find('td:eq(0)').text();
                    myObject.Amount = row.find('td:eq(1)').text();
                    return myObject;
                }
            });
        });

        $(document).ready(function() {
            $(document).on("paintUpdate", function() {
                var paintData = [];
                var paintTotal = 0;
                $('#paint_table tbody tr').each(function(index, value) {
                    var row =  GetPaintRow(index);
                    paintData.push(row);
                });
                $('#paintData').val(JSON.stringify(paintData));
                $('#paint_total_preview').text(paintTotal);
                $(this).trigger("totalUpdate");
                    // Read the row into an object
                function GetPaintRow(rowNum)
                {
                    var row = $('#paint_table tbody tr').eq(rowNum);

                    var myObject = {};
                    var paintCost = row.find('td:eq(1)').text();
                    paintCost = Number(paintCost);
                    paintTotal = Number(paintTotal) + paintCost;
                    paintTotal = paintTotal.toFixed(2);
                    myObject.Description = row.find('td:eq(0)').text();
                    myObject.Amount = row.find('td:eq(1)').text();
                    return myObject;
                }
